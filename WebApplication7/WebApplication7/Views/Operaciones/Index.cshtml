        </th>
        <th>
            @Html.DisplayNameFor(model => model.personas1.nif) vendedor
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.precio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.coche.marca)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.personas.nif)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tipo_operacion.descripcion)
